s = input()
s = s.upper()
s = list(s)
count = {}
for i in range(len(s)):
    if s[i] not in count:
        count[s[i]] = 0
    else:
        count[s[i]] += 1
count_max = max(count.values())
count_n = 0
for value in count.values():
    if value == count_max:
        count_n +=1
if count_n > 1:
    print("?")
else:
    for key, value in count.items():
        if value == count_max:
            print(key)
